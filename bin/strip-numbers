#!/usr/bin/env node

var fs = require("fs");

var data = fs.readFileSync(process.argv[2], 'utf8');
var idx = data.indexOf('{');

var newObj = {};
var origObj = JSON.parse(data.substr(idx));

for (var key in origObj) {
  newObj[key.replace(/^[A-Z]?\d+[.:)]? */, '')] = origObj[key];
}

console.log(data.substr(0, idx) + JSON.stringify(newObj, null, 2));
